/* pages/self-detail/index.wxss */

.self-detail {
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 4px 8px 0px rgba(221, 221, 221, 1);
  border-radius: 20px;
  margin: 20rpx;
}
.self-items{
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
  border-bottom: 1px solid #eee;
  font-size: 28rpx;
  color: #323232;
}
.self-items image{
  width: 58rpx;
  height: 58rpx;
  border-radius: 50%;
}
.self-items text:nth-of-type(2){
  color: #646464;
}
